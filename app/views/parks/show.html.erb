<h1><%= @park.name %> National Park, <%= @park.location %></h1>
<br>


<% @doi_photos["data"].each do |photo| %>
  <% if photo["location"]!= nil && (photo["location"]["name"]).include?(@park.name)%>
    <%= image_tag (photo["images"]["standard_resolution"]["url"]) %>
  <% end %>
<% end %>

<% @photos["data"].each do |photo| %>
<%= image_tag (photo["images"]["standard_resolution"]["url"]) %>
<% end %>
